  163  def update_ticks(matshow, imgfilename, fig)
  164      fig.clf()     ax = fig.add_subplot('111')
  165      plt.imshow(matshow,interpolation='nearest')
  166      ax.imshow(matshow,interpolation='nearest')
  167      ticks = ['HC', 'A-purple','A-orange','B-magenta', 'B-white', 'C-red', 'C-violet']
  168      tickcolors = ['green','purple','orange','magenta','white','red','violet']
  169      ax.set_xticks(range(len(ticks)),ticks)
  170      ax.set_yticks(range(len(ticks)),ticks)
  171      plt.xticks(range(len(ticks)),ticks)
  172      plt.yticks(range(len(ticks)),ticks)
  173      for i in range(len(ax.xaxis.get_major_ticks())):
  174          xtick = ax.xaxis.get_major_ticks()[i]
  175          ytick = ax.yaxis.get_major_ticks()[i]
  176          xtick.label.set_color(tickcolors[i])
  177          xtick.label.set_text(ticks[i])
  178          ytick.label.set_color(tickcolors[i])
  179          xtick.label.set_rotation('vertical')
  180      plt.colorbar()     plt.savefig(imgfilename, facecolor=fig.get_facecolor(), edgecolor='none', bbox_inches='tight')
  181      
